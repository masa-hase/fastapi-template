# Application settings
APP_NAME="FastAPI Template"
APP_VERSION="0.1.0"
DEBUG=false
ENVIRONMENT="development"

# Server settings
HOST="0.0.0.0"
PORT=8000
WORKERS=1

# Logging settings
LOG_LEVEL="INFO"
LOG_FILE="logs/app.log"

# API settings
API_PREFIX="/api/v1"
CORS_ORIGINS=["*"]
CORS_CREDENTIALS=true
CORS_METHODS=["*"]
CORS_HEADERS=["*"]

# Security settings
SECRET_KEY="your-secret-key-here-change-in-production"
ALGORITHM="HS256"
ACCESS_TOKEN_EXPIRE_MINUTES=30